              "example":2
            },
            {
              "in": "query",
              "name": "search",
              "required": false,
              "type": "string",
              "description": "search prodcuts",
              "example": "biriyani"
            },
            {
              "in": "query",
              "name": "category_id",
              "required": false,
              "type": "number",
              "example":1
            },
            {
              "in": "query",
              "name": "status",
              "required": false,
              "type": "string",
              "example": "active"
            }
          ],
          "responses": {
            "200": {
                "description": "successful operation",
                "schema": {
                    "type": "object",
                    "properties": {
                        "message": {
                            "type": "string"
                        }
                    },
                    "example": {
                        "message": "items from products"
                    }
                }
            },
            "400": {
                "description": "Invalid request",
                "schema": {
                    "type": "object",
                    "properties": {
                        "error": {
                            "type": "string"
                        }
                    },
                    "example": {
                        "error": "failed to display cart."
                    }
                }
            },
            "403": {
                "description": "Unauthorized access",
                "schema": {
                    "type": "object",
                    "example": {
                        "error": "Unauthorized access"
                    }
                }
            }
        },
        "security": [
            {
              "cookieAuth":[]
            }
        ]

        }
    },

