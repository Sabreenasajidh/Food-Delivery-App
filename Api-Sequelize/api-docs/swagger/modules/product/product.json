            "operationId": "createproduct",
            "produces": [
                "application/json"
            ],
            "parameters": [],
            "requestBody": {
                "description": "create product body payload",
                "content": {
                    "multipart/form-data": {
                        "schema": {
                            "$ref": "#/definitions/createProduct"                           
                        }
                    }
                }
            },
            "responses": {
                "200": {
                    "description": "successful operation",
                    "schema": {
                        "type": "object",
